pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
---------------------
-- character class --
---------------------
character={
	x=32,
	y=32,
	sprite=048,
	name="default"
}

function character:new(o)
	o=o or {}
	setmetatable(o, self)
	self.__index=self
	return o
end

function character:draw()
	spr(self.sprite,self.x,self.y)
end

characters={}

----------------
-- mouse info --
----------------
mouse={
	x=64,
	y=64,
	a=0,
	b=0,
	hover=false
}
selected=nil

--------------------
-- core functions --
--------------------
function _init()
	for i=1,16 do
		local temp_char=character:new()
		temp_char.x=i*8
		temp_char.y=i*8
		temp_char.name="char"..i
		add(characters,temp_char)
	end
end

function _update()
	if(btnp(0)) mouse.x-=8 --left
	if(btnp(1)) mouse.x+=8 --right
	if(btnp(2)) mouse.y-=8 --up
	if(btnp(3)) mouse.y+=8 --down
	
	for char in all(characters) do
		if(check_col(mouse.x,mouse.y)!=nil) then
			mouse.hover=true
		else
			mouse.hover=false
		end
	end
	
	if(btnp(4)) then
		if(mouse.hover) then
		 selected=check_col(mouse.x,mouse.y)
 	else
 		selected=nil
 	end
	end
	
	if(mouse.x<0) mouse.x=0
	if(mouse.y<0) mouse.y=0
end

function _draw()
 cls()
 camera(mouse.x-64,mouse.y-64)
	map(0,0)
			
	for char in all(characters) do
		char:draw()
	end
	
	if(mouse.hover)	spr(5,mouse.x,mouse.y)
	if(not mouse.hover) spr(4,mouse.x,mouse.y)
	
	if (selected!=nil) then
		camera()
		map(0,40,8,104,8,2,0x80)
		color(0)
		cursor(13,110)
		print(selected.name)
	end
end


--------------------
-- util functions --
--------------------
function check_col(x,y)
	for char in all(characters) do
		if(x>=char.x-4 and x<=char.x+4 and y>=char.y-4 and y<=char.y+4) then
			return char
		end	
	end
	return nil
end
