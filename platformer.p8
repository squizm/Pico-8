pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
--player
player={
	x=8,
	y=8,
	dx=0,
	dy=0,
	ddx=0,
	flipped=false,
	grounded=false,
	sprite=002
}

function _init()
	--low rez
	--poke(0x5f2c,3)
end

function _update()
	player.ddx=0
	
	if(btn(0)) player.ddx+=-0.25
	if(btn(1)) player.ddx+=0.25
	-- if(btn(2)) jump
	-- if(btn(3)) fall through

 -- horizontal accelleration
	player.dx += player.ddx		
	if(player.dx>3) player.dx=3
	if(player.dx<-3) player.dx=-3
	if(player.ddx==0) player.dx*=0.8
	
	-- check grounded
	if collide(player.x/8,player.y/8+1) then
	 player.grounded=true
	else
		player.grounded=false
		player.dy+=0.95
	end
	
	--jump
	if(player.grounded) then
		if(btnp(5))then
			player.dy-=8
		else
			player.dy=0
		end
	end
	
	-- flip spirte	
	if(player.dx>=0) then 
		player.flipped=false
	else
	 player.flipped=true
 end	
 
 -- check horizontal collisions
	while collide((player.x+player.dx)/8,player.y/8) do
		if(player.dx>0) player.dx-=0.01
		if(player.dx<0) player.dx+=0.01
	end
	
	-- check vertical collisions
 while collide(player.x/8,(player.y+player.dy)/8) do
 	if(player.dy>0) player.dy-=0.01
  if(player.dy<0) player.dy+=0.01
 end
 
 
 --move player
 player.x+=player.dx
 player.y+=player.dy
end

function _draw()
	cls()
	map(0,0)
	camera(player.x-64, player.y-64)
	spr(player.sprite,player.x, player.y,1,1,player.flipped,false)
	print(player.dx..','..player.dy,0,-16,8)
	print(player.grounded,0,-8,8)
end

function collide(x,y)
	if(fget(mget(x,y),0)) return true
	return false
end
